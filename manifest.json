{
  "manifest_version": 2,
  "name": "Evermark",
  "description": "A Chrome app that displays your Evernote Bookmarks",
  "version": "0.1",
  "permissions": [
  	"identity",
    "unlimitedStorage",
    "storage",
    "https://www.evernote.com/"  
  ],
  "key": "",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  },
  "options_page": "templates/settings.html"

}